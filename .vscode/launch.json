{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug SVG Editor",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/svg_processor",
            "args": [
                "--parse",
                "${workspaceFolder}/assets/test.svg"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "externalConsole": true,
            "MIMode": "gdb",
            "miDebuggerPath": "C:/Users/sophi/AppData/Roaming/Microsoft/Windows/Start Menu/Programs/MinGW/x86_64-8.1.0-release-posix-seh-rt_v6-rev0/mingw64/bin/gdb.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Fix pretty-printing for gdb",
                    "text": "set charset UTF-8"
                }
            ],
        }
    ],
    "preLaunchTask": "build"
}